{
  "name": "clean-node-api",
  "version": "2.5.4",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/main/server.js",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "postbuild": "copyfiles -u 1 public/**/* dist/static",
    "dev": "sucrase-node src/main/server.ts",
    "up": "yarn build && sudo docker-compose up",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "yarn test -- -c jest-unit-config.js",
    "test:integration": "yarn test -- -c jest-integration-config.js",
    "test:staged": "yarn test -- --findRelatedTests",
    "test:ci": "yarn test -- --coverage",
    "test:coveralls": "yarn test:ci && coveralls < coverage/lcov.info",
    "test:clear:cache": "jest --clearCache"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "1.2.3",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "4.17.6",
    "@types/faker": "^4.1.12",
    "@types/jest": "26.0.13",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "3.5.27",
    "@types/node": "14.6.4",
    "@types/supertest": "2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "13.1.0",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "copyfiles": "^2.3.0",
    "coveralls": "^3.1.0",
    "eslint": "7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard-with-typescript": "19.0.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "11",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "4",
    "eslint-plugin-standard": "4",
    "faker": "^5.1.0",
    "git-commit-msg-linter": "2.6.0",
    "husky": "^4.2.5",
    "jest": "26.4.2",
    "lint-staged": "10.3.0",
    "mockdate": "^3.0.2",
    "prettier": "2.1.1",
    "rimraf": "^3.0.2",
    "sucrase": "^3.15.0",
    "supertest": "^4.0.2",
    "ts-jest": "26.3.0",
    "ts-node-dev": "^1.0.0-pre.50",
    "typescript": "4.0.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "mongo-round": "^1.0.0",
    "mongodb": "3.6.1",
    "nodemon": "^2.0.4",
    "swagger-ui-express": "^4.1.4",
    "validator": "^13.0.0"
  },
  "engines": {
    "node": "12.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
