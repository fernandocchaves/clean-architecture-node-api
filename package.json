{
  "name": "clean-node-api",
  "version": "1.2.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/main/server.js",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy dist/main/server.js",
    "build": "rimraf dist && tsc",
    "dev": "sucrase-node src/main/server.ts",
    "up": "yarn build && sudo docker-compose up",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "yarn test -- -c jest-unit-config.js",
    "test:integration": "yarn test -- -c jest-integration-config.js",
    "test:staged": "yarn test -- --findRelatedTests",
    "test:ci": "yarn test -- --coverage",
    "test:coveralls": "yarn test:ci && coveralls < coverage/lcov.info",
    "test:clear:cache": "jest --clearCache"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.1.5",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.6",
    "@types/jest": "26.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.20",
    "@types/node": "^14.0.5",
    "@types/supertest": "^2.0.9",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "coveralls": "^3.1.0",
    "eslint": "7",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "11",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "4",
    "eslint-plugin-standard": "4",
    "git-commit-msg-linter": "^2.4.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.6",
    "mockdate": "^3.0.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sucrase": "^3.15.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "ts-node-dev": "^1.0.0-pre.50",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.8",
    "nodemon": "^2.0.4",
    "validator": "^13.0.0"
  },
  "engines": {
    "node": "12.x"
  }
}
